number= input("Plese enter a number")
one = 1

if number.isnumeric():
    number = int(number)
    number = number*2
    number = number - 1
    
    for number in range (1, number, 2):
        while number >=1:
            if number == 1:
                print(one)
        print(number)

   
else:
    print("Please try again and enter a whole number")